{
  "version": 2,
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index.py" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
      ]
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py",
    "PYTHONPATH": ".",
    "VERCEL": "1"
  }
}