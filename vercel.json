{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30,
      "maxLambdaSize": "50mb"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py",
    "PYTHONPATH": "."
  }
}